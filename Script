
local library = loadstring(game:GetObjects("rbxassetid://7657867786")[1].Source)()
local Wait = library.subs.Wait 
--------- [ Bloodlen ] -----------
_G.esp = true
_G.removeblind = true
_G.hidderTasks = true
_G.autotask = true
--------- [ Function ] -----------
local plrtaskCheck = game:GetService("Players").LocalPlayer.PlayerGui.TaskUI.Body
function CheckTask()
	local PlrTask = game:GetService("Players").LocalPlayer.PlayerGui.TaskUI.Body:GetChildren()

	for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.TaskUI.Body:GetChildren())  do
		if v:IsA("Frame") then
			if v.Name == "Garden" or v.Name == "Store" or v.Name == "Street" or v.Name == "Beach" then
				if v.Name == "Garden" then
					if v.Garden.TaskText == "Garden | Skrunkly" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(331.148, 12.251, 337.631)
						task.wait(7)
					end

				elseif v.Name == "Garden" then
					if v.Garden.TaskText == "Garden | Goobly" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(306.038, 12.251, 364.407)
						task.wait(7)
					end

				elseif v.Name == "Rock" then
					if v.Garden.TaskText == "Rock | Wave at tha gooberro" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(306.038, 12.251, 364.407)
						task.wait(7)
					end

				elseif v.Name == "Rock" then
					if v.Garden.TaskText == "Rock | Celebratory! yippeee" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(196.923, 21.501, 254.493)
						task.wait(7)
					end

				elseif v.Name == "Beach" then
					if v.Garden.TaskText == "Beach | Do some stuff at the alcove" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(217.75, 8.501, 246.5)
						task.wait(7)
					end

				elseif v.Name == "Beach" then
					if v.Garden.TaskText == "Beach | Relaxin' (the other one)" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(242.75, 8.751, 241)	
						task.wait(7)				
					end

				elseif v.Name == "Beach" then
					if v.Garden.TaskText == "Beach | Haii!! hello!!" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(276.95, 8.501, 240)
						task.wait(7)
					end

				elseif v.Name == "Beach" then
					if v.Garden.TaskText == "Beach | Do some stuff at the alcove" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(217.75, 8.501, 246.5)
						task.wait(7)
					end

				elseif v.Name == "Beach" then
					if v.Garden.TaskText == "Beach | Do some stuff at the alcove" then
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(217.75, 8.501, 246.5)
						task.wait(7)
					end
				end




			end
		else
			return
		end
	end
    task.wait(1)
end

--------- [ Main ] -----------
local MainWindow = library:CreateWindow({
	Name = " dingus | [ Version : 2.3 ]",
	Themeable = {
		Info = "Script Made By Noob"
	}
})
local GeneralTab = MainWindow:CreateTab({
	Name = "General"
})
local Mics = MainWindow:CreateTab({
	Name = "Mics"
})
----------- [ ESP ] -----------
local Esp = GeneralTab:CreateSection({
	Name = " Hunter "
})
Esp:AddToggle({
	Name = "See Hider",
	Flag = "As The Hunter This Useful",
	Callback = function(value)
		local Players = game:GetService("Players"):GetChildren()
		local RunService = game:GetService("RunService")
		local highlight = Instance.new("Highlight")
		highlight.Name = "Highlight"
		_G.esp = value
		if _G.esp == false then 
			for i, v in pairs(Players) do
				repeat wait() until v.Character
				if v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
					v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight"):Destroy()
				end
			end
		else
			while _G.esp == true do
				wait()
				for i, v in pairs(Players) do
					repeat wait() until v.Character
					if not v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
						local highlightClone = highlight:Clone()
						highlightClone.Adornee = v.Character
						highlightClone.Parent = v.Character:FindFirstChild("HumanoidRootPart")
						highlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
						highlightClone.Name = "Highlight"
					end
				end

				game.Players.PlayerAdded:Connect(function(player)
					repeat wait() until player.Character
					if not player.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
						local highlightClone = highlight:Clone()
						highlightClone.Adornee = player.Character
						highlightClone.Parent = player.Character:FindFirstChild("HumanoidRootPart")
						highlightClone.Name = "Highlight"
					end
				end)

				game.Players.PlayerRemoving:Connect(function(playerRemoved)
					playerRemoved.Character:FindFirstChild("HumanoidRootPart").Highlight:Destroy()
				end)

				RunService.Heartbeat:Connect(function()
					for i, v in pairs(Players) do
						repeat wait() until v.Character
						if not v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
							local highlightClone = highlight:Clone()
							highlightClone.Adornee = v.Character
							highlightClone.Parent = v.Character:FindFirstChild("HumanoidRootPart")
							highlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
							highlightClone.Name = "Highlight"
							task.wait()
						end
					end
				end)
			end

		end




	end
})

Esp:AddButton({
	Name = "Fix Esp",
	Callback = function()
		while true do
			wait()
			local Players = game:GetService("Players"):GetChildren()
			local highlight = Instance.new("Highlight")
			for i, v in pairs(Players) do
				repeat wait() until v.Character
				if not v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
					local highlightClone = highlight:Clone()
					highlightClone.Adornee = v.Character
					highlightClone.Parent = v.Character:FindFirstChild("HumanoidRootPart")
					highlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
					highlightClone.Name = "Highlight"
				end
			end

			game.Players.PlayerAdded:Connect(function(player)
				repeat wait() until player.Character
				if not player.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
					local highlightClone = highlight:Clone()
					highlightClone.Adornee = player.Character
					highlightClone.Parent = player.Character:FindFirstChild("HumanoidRootPart")
					highlightClone.Name = "Highlight"
				end
			end)

			game.Players.PlayerRemoving:Connect(function(playerRemoved)
				playerRemoved.Character:FindFirstChild("HumanoidRootPart").Highlight:Destroy()
			end)

			RunService.Heartbeat:Connect(function()
				for i, v in pairs(Players) do
					repeat wait() until v.Character
					if not v.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
						local highlightClone = highlight:Clone()
						highlightClone.Adornee = v.Character
						highlightClone.Parent = v.Character:FindFirstChild("HumanoidRootPart")
						highlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
						highlightClone.Name = "Highlight"
						task.wait()
					end
				end
			end)
		end

	end





})

Esp:AddToggle({
	Name = "Remove Blind",
	Flag = "As The Hunter This Useful",
	Callback = function(v)
		_G.removeblind = v

		while _G.removeblind == true do
			wait()
			game:GetService("Players").LocalPlayer.PlayerGui.BlindnessGui.Frame.BackgroundTransparency = 1
			warn("Blindness Gui has been removed ,Clear View For Hunter")
		end

	end
})

----------- [ Hidder ] -----------
local Hidder = GeneralTab:CreateSection({
	Name = " Hider "
})
Hidder:AddToggle({
	Name = "Auto Task",
	Flag = "Just_Need_Hold_E",
	Callback = function(v)
		_G.autotask = v
		if not _G.autotask then return end
		repeat CheckTask() until plrtaskCheck.Garden == nil and plrtaskCheck.Store == nil or plrtaskCheck.Street == nil or plrtaskCheck.Beach == nil
		warn("Task Done!")

	end
})
