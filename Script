local localplayerSec = GeneralTab:CreateSection({
	Name = "Player"


})
localplayerSec:AddSlider({
	Name = "PLayer Speed",
	Flag = "Speed",
	Value = 16,
	Min = 16,
	Max = 200,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end
})
